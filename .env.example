# ByteSip Local Development Environment Variables
# Copy this file to .env and fill in your values

# ============================================
# DynamoDB Local Configuration
# ============================================
DYNAMODB_TABLE_NAME=bytesip-news-cache
DYNAMODB_ENDPOINT_URL=http://localhost:8000
AWS_REGION=ap-northeast-1

# ============================================
# External API Tokens
# ============================================
# Qiita API Access Token
# Generate at: https://qiita.com/settings/tokens/new
QIITA_ACCESS_TOKEN=your_qiita_access_token_here

# GitHub Personal Access Token
# Generate at: https://github.com/settings/tokens
GITHUB_ACCESS_TOKEN=your_github_access_token_here
